{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "noUncheckedIndexedAccess": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.cjs",
    "**/*.mjs",
    "_src/components/nurturing/Nurturing.js",
    "_src/components/effect/EffectSlider.js",
    "_src/components/common/List.tsx",
    "_src/components/nurturing/NurturingMo.js",
    "tailwind.config.js",
    "_src/components/deck/RecommendDecks.js",
    "_src/components/deck/RecommendDecks.js",
    "next.config.cjs",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "_src/**/*.js"
  ]
}
