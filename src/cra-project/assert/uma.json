---结构体---
{
  "card":{
    "basic":{
      "title":"基础信息",
      "data":[
        {"key":"id","title":"id","type":"text"},
        {"key":"rarity","title":"稀有度","type":"card_rarity"},
        {"key":"name","title":"名字","type":"text"},
        {"key":"name2","title":"昵称","type":"text"},
        {"key":"image","title":"图片","type":"image"}
      ]
    },
    "ground":{
      "title":"场地适应",
      "data":[
        {"key":"ground_turf","title":"草地","type":"id","data_source":"card_adapt"},
        {"key":"ground_dirt","title":"泥地","type":"id","data_source":"card_adapt"}
      ]
    },
    "distance":{
      "title":"距离适应",
      "data":[
        {"key":"distance_short","title":"短距离","type":"id","data_source":"card_adapt"},
        {"key":"distance_mile","title":"英里","type":"id","data_source":"card_adapt"},
        {"key":"distance_middle","title":"中距离","type":"id","data_source":"card_adapt"},
        {"key":"distance_long","title":"长距离","type":"id","data_source":"card_adapt"}
      ]
    },
    "running_style":{
      "title":"跑法适应",
      "data":[
        {"key":"running_style_nige","title":"逃","type":"id","data_source":"card_adapt"},
        {"key":"running_style_senko","title":"先","type":"id","data_source":"card_adapt"},
        {"key":"running_style_sashi","title":"差","type":"id","data_source":"card_adapt"},
        {"key":"running_style_oikomi","title":"追","type":"id","data_source":"card_adapt"}
      ]
    },
    "attr_grow":{
      "title":"属性成长",
      "data":[
        {"key":"speed","title":"速度","type":"number"},
        {"key":"stamina","title":"耐力","type":"number"},
        {"key":"pow","title":"力量","type":"number"},
        {"key":"guts","title":"毅力","type":"number"},
        {"key":"wiz","title":"智力","type":"number"}
      ]
    },
    "skill_group":{
      "title":"技能",
      "data":[
        {"key":"skills1","title":"被动技能","type":"id_list","data_source":"skill"},
        {"key":"skills2","title":"主动技能","type":"id_list","data_source":"skill"}
      ]
    },
    "event_group":{
      "title":"事件",
      "data":[
        {"key":"events1","title":"事件","type":"id_list","data_source":"event"}
      ]
    }
  },
  "card_rarity":{
    "basic":{
      "title":"稀有度",
      "data":[
        {"key":"id","title":"id","type":"text"},
        {"key":"title","title":"名字","type":"text"}
      ]
    }
  },
  "card_adapt":{
    "basic":{
      "title":"适应性",
      "data":[
        {"key":"id","title":"id","type":"text"},
        {"key":"title","title":"名字","type":"text"}
      ]
    }
  },
  "skill":{
    "basic":{
      "title":"技能信息",
      "data":[
        {"key":"id","title":"id","type":"text"},
        {"key":"rarity","title":"稀有度","type":"skill_rarity"},
        {"key":"name","title":"名字","type":"text"},
        {"key":"image","title":"图片","type":"image"},
        {"key":"cool_down","title":"冷却时间","type":"number"},
        {"key":"need_skill_point","title":"学习价格","type":"number"},
        {"key":"grade_value","title":"评价分","type":"number"},
        {"key":"ability_time","title":"持续时间","type":"number"},
        {"key":"ability","title":"效果","type":"id_list","data_source":"skill_ability"}
      ]
    }
  },
  "skill_ability":{
    "basic":{
      "title":"技能效果",
      "data":[
        {"key":"type","title":"类型","type":"id","data_source":"skill_ability_type"},
        {"key":"value","title":"数值","type":"number"}
      ]
    }
  },
  "event":{
    "basic":{
      "title":"事件信息",
      "data":[
        {"key":"id","title":"id","type":"text"},
        {"key":"name","title":"名字","type":"text"},
        {"key":"choices","title":"选项","type":"id_list","data_source":"event_choice"}
      ]
    }
  },
  "event_choice":{
    "basic":{
      "title":"选项",
      "data":[
        {"key":"id","title":"id","type":"text"},
        {"key":"name","title":"名字","type":"text"},
        {"key":"results","title":"结果","type":"text"}
      ]
    }
  }
}

